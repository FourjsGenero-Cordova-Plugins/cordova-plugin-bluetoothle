LAYOUT
GRID
{
[bs                           ]
[lstate       |stat           ]
[laddr        |addr           ]
[ltmst        |tmst           ]
[ltemp        |temp           ]
[bq                           ]
}
END
ATTRIBUTES
BUTTON bs: start, TEXT="Start test";
BUTTON bq: cancel, TEXT="End test";
LABEL lstate:TEXT="Process state";
LABEL laddr:TEXT="Tag address";
LABEL ltmst:TEXT="Time stamp";
LABEL ltemp:TEXT="Current Temp";
EDIT stat=FORMONLY.state, SCROLL, NOENTRY, PLACEHOLDER="";
EDIT addr=FORMONLY.address, SCROLL, NOENTRY, PLACEHOLDER="";
EDIT tmst=FORMONLY.timestamp,SCROLL, NOENTRY, PLACEHOLDER="";
EDIT temp=FORMONLY.temperature, PLACEHOLDER="";
END
