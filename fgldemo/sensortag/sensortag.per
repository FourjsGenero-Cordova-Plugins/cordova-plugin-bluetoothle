LAYOUT
GRID
{
[bs                           ]
[lstat        |stat           ]
[ldevi        |devi           ]
[laddr        |addr           ]
[ltmst        |tmst           ]
[ltmpf        |tmpf           ]
[ltmpc        |tmpc           ]
[bq                           ]
}
END
ATTRIBUTES
BUTTON bs: start, TEXT="Start test";
BUTTON bq: cancel, TEXT="End test";
LABEL lstat:TEXT="State";
LABEL ldevi:TEXT="Device";
LABEL laddr:TEXT="Address";
LABEL ltmst:TEXT="Timestamp";
LABEL ltmpf:TEXT="Temp (Far.)";
LABEL ltmpc:TEXT="Temp (Cel.)";
BUTTONEDIT stat=FORMONLY.state, SCROLL, PLACEHOLDER="", NOTEDITABLE;
EDIT devi=FORMONLY.device, SCROLL, NOENTRY, PLACEHOLDER="";
EDIT addr=FORMONLY.address, SCROLL, NOENTRY, PLACEHOLDER="";
EDIT tmst=FORMONLY.timestamp, SCROLL, NOENTRY, PLACEHOLDER="";
EDIT tmpf=FORMONLY.tempfar, NOENTRY, PLACEHOLDER="";
EDIT tmpc=FORMONLY.tempcel, NOENTRY, PLACEHOLDER="";
END
